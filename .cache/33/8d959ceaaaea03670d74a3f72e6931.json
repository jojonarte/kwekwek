umn":0},"source":"js/renderer.js","original":{"line":49,"column":3}},{"generated":{"line":98,"column":6},"source":"js/renderer.js","original":{"line":50,"column":2}},{"name":"Highway","generated":{"line":103,"column":2},"source":"js/renderer.js","original":{"line":19,"column":23}},{"name":"Renderer","generated":{"line":103,"column":19},"source":"js/renderer.js","original":{"line":19,"column":31}},{"generated":{"line":103,"column":30},"source":"js/renderer.js","original":{"line":53,"column":0}},{"name":"Renderer","generated":{"line":106,"column":15},"source":"js/renderer.js","original":{"line":54,"column":15}}],"sources":{"js/renderer.js":"import Highway from '@dogstudio/highway';\r\nimport PerfectScrollbar from 'perfect-scrollbar';\r\n\r\nconst triggerPageAnimation = () => {\r\n  document.querySelectorAll('.animated').forEach(d => {\r\n    let willAnimate = true;\r\n    d.classList.forEach(c => {\r\n      if (c.indexOf('animate__delay') > -1) {\r\n        willAnimate = false;\r\n      }\r\n    });\r\n    if (willAnimate) {\r\n      d.classList.add('animate__animated');\r\n    }\r\n    d.classList.remove('animated');\r\n  });\r\n}\r\n\r\nclass Renderer extends Highway.Renderer {\r\n  // Hooks/methods\r\n  onEnter() {\r\n    const container = this.wrap.querySelector('.contentWrapper');\r\n    const ps = new PerfectScrollbar(container, {\r\n      wheelSpeed: 2,\r\n      wheelPropagation: true,\r\n      minScrollbarLength: 20\r\n    });\r\n    const slug = this.properties.slug\r\n    document.querySelector(`[data-slug=${slug}]`).classList.add('active');\r\n    document.title = `${slug} - kwekwek template`;\r\n  }\r\n  onEnterCompleted() {\r\n    triggerPageAnimation();\r\n\r\n    const container = this.wrap.querySelector('.contentWrapper');\r\n    const vh = Math.max(document.documentElement.clientHeight || 0, window.innerHeight || 0)\r\n    container.addEventListener('ps-scroll-y', (e) => {\r\n      if (e.target.scrollTop >= (vh / 2)) {\r\n        document.body.classList.add('page-scrolled');\r\n      } else {\r\n        document.body.classList.remove('page-scrolled');\r\n      }\r\n    })\r\n  }\r\n  onLeave() {\r\n    document.querySelectorAll('.navLink').forEach(o => {\r\n      o.classList.remove('active');\r\n    })\r\n  }\r\n  // onLeaveCompleted() {}\r\n}\r\n\r\n// Don`t forget to export your renderer\r\nexport default Renderer;"},"lineCount":null}},"error":null,"hash":"344036aebda11ad1fac6af743fc29b9c","cacheData":{"env":{}}}